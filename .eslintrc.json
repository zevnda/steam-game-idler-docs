{
    "extends": "next/core-web-vitals",
    "env": {
        "es6": true
    },
    "rules": {
        "strict": [
            "error"
        ],
        "no-undef": [
            "error"
        ],
        "no-unused-expressions": [
            "error"
        ],
        "no-multiple-empty-lines": [
            "warn"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "warn",
            "always"
        ],
        "no-unreachable": [
            "error"
        ],
        "no-unused-vars": [
            "error"
        ],
        "no-sync": [
            "warn"
        ],
        "react-hooks/exhaustive-deps": [
            "off"
        ]
    }
}